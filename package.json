{
  "name": "recipe-rest-api",
  "version": "1.0.0",
  "description": "A sandbox express project",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "migrate": "npx ts-node ./node_modules/knex/bin/cli.js --knexfile='./src/knexfile.ts' migrate:latest",
    "run": "npx nodemon --inspect=0.0.0.0:9229 dist/app.js",
    "run prod": "node dist/app.js",
    "test": "test",
    "watch": "tsc -w"
  },
  "author": "Ryan Brown",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^4.0.1",
    "commander": "^5.0.0",
    "connect-pg-simple": "^6.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "knex": "^0.20.10",
    "objection": "^2.1.3",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^7.18.2",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/connect-pg-simple": "^4.2.0",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.2",
    "@types/express-session": "^1.17.0",
    "@types/passport": "^1.0.3",
    "@types/passport-local": "^1.0.33",
    "debug": "^4.1.1",
    "nodemon": "^2.0.2"
  }
}
